@inherits ComponentBase_WithGettextSupport;
<div class="alert alert-secondary mt-4" role="alert">
    <span class="fa fa-pencil mr-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
        @TextPleaseTakeOur
        <a target="_blank" class="font-weight-bold" href="https://go.microsoft.com/fwlink/?linkid=2137813">@TextBriefSurvey</a>
    </span>
    @TextAndTellUsWhatYouThink
</div>

@code {
    // Demonstrates how a parent component can supply parameters
    [Parameter]
    public string Title { get; set; }
}
