@if (Pane?.Key != Key || IsEmpty)
{
    return;
}

<div class="card-header">
    <h2 class="mb-0">
        <button class="btn btn-link btn-block text-left" type="button" @onclick="@(e => Parent.HandleTabClick(Pane))">
            @Pane.Tab
        </button>
    </h2>
</div>
@if (Pane.IsActive)
{
    <div class="collapse show">
        <div class="card-body">
            @Pane.ChildContent
        </div>
    </div>
}