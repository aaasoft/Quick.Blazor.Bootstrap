@if (Pane?.Key != Key || IsEmpty)
{
    return;
}
@if (IsTab)
{
    if (Pane.IsActive)
    {
        <a class="nav-link active" href="javascript:;" aria-selected="true">@Pane.Tab</a>
    }
    else
    {
        <a class="nav-link" href="javascript:;" aria-selected="false" @onclick="@(e => Parent.HandleTabClick(Pane))">@Pane.Tab</a>
    }
}
else if (IsPane)
{
    if (Pane.IsActive)
    {
        <div class="tab-pane fade show active" role="tabpanel">@Pane.ChildContent</div>
    }
}