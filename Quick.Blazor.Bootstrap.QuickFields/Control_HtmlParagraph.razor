@using Quick.Fields

<p class="@ControlUtils.GetCommonClass(Field)" style="@Field.Html_Style">@Field.Value
    @if (Field.Children != null)
    {
        @foreach (var child in Field.Children)
        {
            <Control Field="@child" />
        }
    }
</p>

@code
{
    [Parameter]
    public FieldForGet Field { get; set; }
}